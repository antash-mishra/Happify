<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.happify">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Happify">
        <activity android:name=".DashboardActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".Read" />
        <activity android:name=".Test" />
        <activity android:name=".Depression" />
        <activity android:name=".Addiction" />
        <activity android:name=".Timer" />
        <activity android:name=".Anxiety" />
        <activity android:name=".PTSD" />
        <activity android:name=".Breathe" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>